<div class="p-4 col-6 float-left">
  <%= simple_form_for User.new, url: seekers_path, method: :post do |f| %>
    <%= f.input :email, required: true, required: false %>
    <%#= f.input :password, required: true %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :description %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Add Seeker", class: "btn btn-primary" %>
  <% end %>
</div>
